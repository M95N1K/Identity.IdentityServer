@using Identity.IdentityServer.Models.Entity
@using Microsoft.AspNetCore.Identity
@using System.Linq

@model IEnumerable<UserInfo>
@{
    ViewData["Title"] = "Administrator";
}

@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
        <div class="col-auto border-dark border">
            <p class="card-columns">
                <b>Id:</b><br> @item.Id
            </p>
            <p class="card-columns">
                <b>User Name:</b><br> @item.UserName
            </p>
            <p class="card-columns">
                <b>Given Name:</b><br> @item.GivenName
            </p>
            <p class="card-columns">
                <b>Role:</b><br> @item.Role
            </p>
        </div>
        <hr />
    }
}

